    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                    // "/mnt/2020-02-12-16-47-50.bag", 
                    "/mnt/stereo_calibr_mono.bag",
                    "--topics", 
                    // "/stereo/left/image_raw",
                    // "/stereo/right/image_raw"
                    "/stereo_node/left_image",
                    "/stereo_node/right_image"
                    ]
        }
    ]
